program writeandwriteln; //judul program
uses crt; 
begin
	//fungsi write ialah menuliskan sesuatu di baris yang lama
	write('Hello');
	write('Prosoltion');
	//akhir dari write
	writeln('');
	writeln(''); //memisahkan antara write dan writeln
	//fungsi writeln ialah menuliskan sesuatu di baris yang baru
	writeln('Hello');
	writeln('Prosoltion');
	//akhir dari writeln
	readln(); //agar program tidak langsung tertutup
end.